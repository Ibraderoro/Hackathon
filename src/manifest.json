{
    "name": "YouTube Downloader",
    "version": "1.0",
    "manifest_version": 3,

    "description": "Download from YouTube",
    "action": {
        "default_title": "Download from YouTube",
        "default_popup": "popout.html"
    },
    "permissions": [
        "downloads",
        "activeTab",
        "declarativeContent"
    ],
   "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}